# Simple batch script that starts SCREEN.
#SBATCH --get-user-env
#SBATCH --export=NONE
unset SLURM_EXPORT_ENV
set -x
tmux new-session -d -s slurm$SLURM_JOB_ID
while tmux list-sessions; do
	sleep 5
done


